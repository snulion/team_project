<div class="container">
    <div class="row">
        <div class="col s12 m7">
            <% @posts.each do |p| %>
            <div class="card">
                <div class="card-image">
                  <img src=<%= asset_path("hiphop.png") %>>
                  <span class="card-title"><%= p.genre %></span>
                </div>
                <div class="card-content">
                  <p><%= p.content %></p>
                </div>
                <div class="card-action">
                  <a href="#">This is a link</a>
                </div>
            </div>
            <% end %>
        </div>
    </div>
</div>